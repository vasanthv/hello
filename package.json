{
	"name": "ahey",
	"version": "2.0.0",
	"description": "A free group video conference app for the web. No sign ups. No downloads.",
	"main": "init.js",
	"scripts": {
		"start": "node ./scripts/pre-start.js && node init.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"format:ejs": "prettier --write \"views/**/*.ejs\""
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"ejs": "^3.1.10",
		"express": "^4.19.2",
		"socket.io": "^4.7.5",
		"vue": "^3.4.31",
		"webrtc-adapter": "^9.0.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.12.0",
		"prettier": "3.4.2"
	},
	"prettier": {
		"printWidth": 120,
		"singleQuote": false,
		"trailingComma": "es5",
		"tabWidth": 2,
		"useTabs": true,
		"overrides": [
			{
				"files": [
					"*.ejs"
				],
				"options": {
					"parser": "html"
				}
			}
		]
	}
}
